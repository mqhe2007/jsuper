{
  "source": {
    "include": ["src/"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "sourceType": "module",
  "opts": {
    "readme": "README.md",
    "destination": "./docs/",
    "template": "node_modules/better-docs"
  },
  "templates": {
    "search": true,
    "better-docs": {
      "name": "Jsuper Documentation",
      "title": "Jsuper",
      "hideGenerator": false,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/mqhe2007/jsuper"
        }
      ]
    }
  }
}
